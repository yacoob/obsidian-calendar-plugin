{
  "name": "obsidian-calendar-wrapper-plugin",
  "version": "0.0.1",
  "description": "Wraps https://fullcalendar.io for use in dataview JS blocks",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "cp-plugin-files": "d=$(cat .plugin-dest-dir 2>/dev/null || echo './dist') && npm run build && mkdir -p ${d} && cp -f manifest.json main.js styles.css ${d}",
    "clean": "rm -rf main.js styles.css dist"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@event-calendar/core": "^3.12.0",
    "@event-calendar/day-grid": "^3.12.0",
    "@event-calendar/interaction": "^3.12.0",
    "@event-calendar/list": "^3.12.0",
    "@event-calendar/time-grid": "^3.12.0",
    "@types/event-calendar__core": "^3.7.0",
    "@types/event-calendar__day-grid": "^3.4.1",
    "@types/event-calendar__list": "^3.4.1",
    "@types/event-calendar__time-grid": "^3.4.1",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
